{
  "presets": [
    [
      "env",
      {
        "targets": {
          "browsers": ["last 3 versions"]
        },
        "modules": false,
        "useBuiltIns": false
      }
    ],
    "react"
  ],
  "plugins": [
    "transform-runtime",
    "external-helpers",
    "transform-class-properties"
  ]
}
