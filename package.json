    "jest": "^22.1.4",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-test-renderer": "^16.2.0",
    "regenerator-runtime": "^0.11.1",
    "rollup": "^0.52.3",
    "rollup-plugin-auto-external": "^1.0.0",
