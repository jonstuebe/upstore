  "files": ["lib/"],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jonstuebe/upstore.git"
  },
  "keywords": ["react", "javascript", "redux", "store"],
  "author": "Jon Stuebe <jstuebe@gmail.com>",
  "license": "MIT",
  "bugs": {
